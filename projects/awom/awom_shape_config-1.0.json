{
    "title": "AWOM geojson",
    "type": "object",
    "required": ["type", "geometry", "properties"],
    "properties": {
        "type": {
            "const": "Feature"
        },
        "geometry": {
            "type": "object",
            "required": ["type", "coordinates"],
            "properties": {
                "type": {
                    "const": "Point"
                },
                "coordinates": {
                    "type": "array",
                    "minItems": 2,
                    "maxItems": 2,
                    "items": { "type": "number" }
                }
            },
            "additionalProperties": false
        },
        "properties": {
            "type": "object",
            "maxProperties": 0
        }
    },
    "additionalProperties": false
}
